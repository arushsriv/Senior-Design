{
    "name": "Budgify",
    "description": "Budgify",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
        "storage",
        "activeTab"
    ],
    "host_permissions": [
        "http://localhost:3000/*"
    ],
    "action": {
        "default_popup": "login.html",
        "default_icon": "hello_extensions.png"
    }
}